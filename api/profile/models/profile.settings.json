{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "profile"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bio": {
      "type": "text"
    },
    "profilePictureUrl": {
      "type": "text"
    },
    "socialMediaLinks": {
      "type": "component",
      "repeatable": true,
      "component": "links.social-media-links"
    },
    "level": {
      "type": "integer"
    },
    "user": {
      "via": "profile",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
