{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "catchPhrase": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "heroImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "vision": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "commitmentLevel": {
      "type": "string",
      "required": true
    },
    "signupFormUrl": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "meetingLinkURLs": {
      "type": "component",
      "repeatable": true,
      "component": "links.meeting-link-ur-ls",
      "required": true
    },
    "meetingTimes": {
      "type": "component",
      "repeatable": true,
      "component": "dates.meeting-times",
      "required": true
    },
    "keywords": {
      "type": "component",
      "repeatable": true,
      "component": "keywords.keywords",
      "required": true
    },
    "projectReferenceURLs": {
      "type": "component",
      "repeatable": true,
      "component": "links.project-reference-ur-ls",
      "required": false
    },
    "openPositions": {
      "type": "component",
      "repeatable": true,
      "component": "positions.open-positions",
      "required": false
    },
    "team": {
      "type": "component",
      "repeatable": false,
      "component": "people.people",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "projects"
    },
    "Images": {
      "type": "component",
      "repeatable": true,
      "component": "links.images"
    },
    "interests": {
      "via": "projects",
      "collection": "interest"
    },
    "calendarId": {
      "type": "string"
    },
    "board": {
      "type": "component",
      "repeatable": false,
      "component": "project.project-board"
    },
    "isListed": {
      "type": "boolean",
      "required": true
    },
    "subProjects": {
      "collection": "project",
      "via": "parentProject"
    },
    "parentProject": {
      "model": "project",
      "via": "subProjects"
    }
  }
}
